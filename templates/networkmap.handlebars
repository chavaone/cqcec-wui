<ul id="conn_ips">
	{{#each connections}}
	<li id="conn{{@index}}" class="connected_ip" onclick="app.enable_ip_info('{{@key}}')">
		<span class="moonicon-screen type_icon"></span>
		<ul class="ip_info">
			<li class="ip">{{@key}}</li>
			<li class="hostname">{{hostname}} </li>
			<li class="macvendor">{{vendor}} </li>
		</ul>
		<ul class="numconn">
			<li>
				<span class="glyphicon glyphicon-chevron-up pull-left"></span>
				Sa√≠ntes
				<span class="badge pull-right">{{{connnum this.Outgoing}}}</span>
			</li>
			<li>
				<span class="glyphicon glyphicon-chevron-down pull-left"></span>
				Entrantes
				<span class="badge pull-right">{{{connnum this.Incoming}}}</span>
			</li>
		</ul>
	</li>
	{{/each}}
</ul>