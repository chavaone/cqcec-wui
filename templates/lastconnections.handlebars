<ul id="lastconnections">
	{{#each connections}}
	<li id="conn{{@index}}" class="well well-sm connection" onclick="app.show_ip_last_connection_details('{{@index}}', '{{this.ip_origen}}', '{{this.ip_dest}}')">
		<div class="brief">
			<ul class="ip">
				<li class="ip_add">{{this.ip_origen}}</li>
				<li class="domain">
					<span class="domain">
						{{#if this.orig_islocal}}
							(local)</span>
						{{else}}
							{{#if this.domain_orig}}
								({{this.domain_orig}})
							{{else}}
								(unknown)
							{{/if}}
							</span>
							<a class="icon-pencil edit_button" href="javascript:void(0)" onclick="app.edit_domain_cache('{{this.ip_dest}}', '{{this.domain_orig}}')"></a>
						{{/if}}
				</li>
			</ul>
			<span class="icon-chevron-sign-right"></span>
			<span class="proto">{{this.proto}}</span>
			<span class="icon-chevron-sign-right"></span>
			<ul class="ip">
				<li class="ip_add">{{this.ip_dest}}</li>
				<li class="domain">
					<span class="domain">
						{{#if this.dest_islocal}}
							(local)</span>
						{{else}}
							{{#if this.domain_dest}}
								({{this.domain_dest}})
							{{else}}
								(unknown)
							{{/if}}
							</span>
							<a class="icon-pencil edit_button" href="javascript:void(0)" onclick="app.edit_domain_cache('{{this.ip_dest}}', '{{this.domain_dest}}')"></a>
						{{/if}}
				</li>
			</ul>
			{{#if this.number}}
			<span class="badge number_badge pull-right">{{this.number}}</span>
			{{/if}}
			<span class="time">({{time this.time}})</span>
		</div>
		<div class="more">
	 		<hr>
	 		<div class="details">
				<i class="icon-spin icon-refresh refresh-icon"></i>
			</div>
		</div>
		</li>
	{{else}}
		<h4>Non hai conexi√≥ns.</h4>
	{{/each}}
 </ul>